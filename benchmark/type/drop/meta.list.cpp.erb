#include <meta/meta.hpp>

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = meta::list<
    <%= ((k*n+1)..(k*n+n)).map { |i| "meta::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = meta::drop<<%= "xs#{k}" %>, meta::size_t<<%= k*n/env[:k] %>>>;
#endif

<% end %>

int main() {}
