#include <boost/tmp.hpp>
namespace tmp = boost::tmp;

template<int> struct x;
template<int> struct y;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = tmp::list_<<%= ((k*n+1)..(k*n+n)).map { |i| "x<#{i}>" }.join(', ') %>>;
using <%= "ys#{k}" %> = tmp::list_<<%= ((k*n+1)..(k*n+n)).map { |i| "y<#{i}>" }.join(', ') %>>;

#if defined(METABENCH)
using <%= "result#{k}" %> = tmp::call_<tmp::product_<>, <%= "xs#{k}" %>, <%= "ys#{k}" %>>;
#endif

<% end %>

int main() {}
