#include <boost/tmp.hpp>
namespace tmp = boost::tmp;

template<typename X>
using f = X;

<% env[:k].times do |k| %>

using <%= "xs#{k}" %> = tmp::list_<
    <%= ((k*n+1)..(k*n+n)).map { |i| "tmp::int_<#{i}>" }.join(', ') %>
>;

#if defined(METABENCH)
using <%= "result#{k}" %> = tmp::call_<tmp::transform_<tmp::lift_<f>>, <%= "xs#{k}" %>>;
#endif

<% end %>

int main() {}
